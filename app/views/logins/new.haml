= render 'shared/error_message'
.row
  .well.col-md-6.col-md-offset-3
    = form_tag '/logins' do
      = label_tag :name
      = text_field_tag :name
      = label_tag :password
      = password_field_tag :password
      = submit_tag 'login', class: "btn btn-success"
= link_to "Create user", new_user_path